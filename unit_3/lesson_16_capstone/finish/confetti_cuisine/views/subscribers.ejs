<div class="container">
  <h1>All Subscribers</h1>

  <div class="list-container">
    <% subscribers.forEach(s => { %>
      <div class="subscriber">
        <p><span class="item-label">Name:</span> <%= s.name %></p>
        <p><span class="item-label">Email:</span> <%= s.email %></p>
        <p><span class="item-label">Zip Code:</span> <%= s.zipCode %></p>
        <p><span class="item-label">Street Address:</span> <%= s.streetAddress %></p>
        <p><span class="item-label">VIP:</span> <%= s.vip ? "Yes" : "No" %></p>
      </div>
      <hr>
    <% }); %>
  </div>
</div>
